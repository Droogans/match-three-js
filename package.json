{
  "name": "match-three",
  "version": "0.0.1",
  "description": "Basic logic for implementing a match three game.",
  "main": "index.js",
  "dependencies": {
    "collections": "^3.0.0",
    "lodash": "4.6.1"
  },
  "devDependencies": {
    "ava": "^0.12.0",
    "babel-cli": "6.6.4",
    "babel-plugin-transform-es2015-destructuring": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "browser-sync": "^2.12.10",
    "browserify": "^13.0.1",
    "remarkable": "^1.6.2"
  },
  "scripts": {
    "start": "scripts/server.js",
    "deploy": "npm run babel && npm run browserify && node site/scripts/generate_readme.js && cp ./site/*.html ./site-gh-pages && cp ./site/*.css ./site-gh-pages",
    "babel": "babel ./babel/ -d .",
    "browserify": "browserify site/scripts/main.js > site-gh-pages/bundle.js",
    "test": "ava",
    "test-es5": "ava"
  },
  "keywords": [
    "match-three",
    "match",
    "three",
    "game"
  ],
  "author": "Andrew Yurisich <droogans@gmail.com> (http://andrew.yurisich.com/)",
  "license": "MIT",
  "ava": {
    "files": [
      "babel/test/**/*.js",
      "!babel/test/data/**"
    ],
    "failFast": true,
    "tap": true,
    "require": [
      "babel-register"
    ]
  }
}
